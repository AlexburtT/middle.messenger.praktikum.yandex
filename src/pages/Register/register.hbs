<main>
	<section class="formData">
		<h2>RocketSend</h2>
		<form>
			<fieldset>
				<legend>Регистрация</legend>
				<div class="inputContainer">
					{{{ first_name }}}
					<label for="login" class="label">Имя</label>
				</div>
				<div class="inputContainer">
					{{{ second_name }}}
					<label for="login" class="label">Фамилия</label>
				</div>
				<div class="inputContainer">
					{{{ login }}}
					<label for="login" class="label">Логин</label>
				</div>
				<div class="inputContainer">
					{{{ email }}}
					<label for="login" class="label">E-mail</label>
				</div>
				<div class="inputContainer">
					{{{ password }}}
					<label for="login" class="label">Пароль</label>
				</div>
				<div class="inputContainer">
					{{{ phone }}}
					<label for="login" class="label">Телефон</label>
				</div>
				{{{ button }}}
			</fieldset>
		</form>
	</section>
</main>
